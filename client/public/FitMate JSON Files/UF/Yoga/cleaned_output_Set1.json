        },
        "mechanic": {
            "name": "Compound",
            "description": "Exercise trains multiple body parts."
        },
        "long_form_content": [],
        "correct_steps": [
            {
                "order": 1,
                "text": "Begin on your hands and knees with your hands shoulder-width apart and your knees hip-width apart."
            },
            {
                "order": 2,
                "text": "Spread your fingers wide and press down into your palms, making sure that your fingers are evenly spaced and facing forward."
            },
            {
                "order": 3,
                "text": "Tuck your toes under and lift your knees off the ground, straightening your legs as much as possible."
            },
            {
                "order": 4,
                "text": "Press your hips up and back, bringing your body into an inverted \"V\" shape."
            },
            {
                "order": 5,
                "text": "Keep your arms straight and your shoulder blades spread apart, and try to press your heels down towards the ground."
            },
            {
                "order": 6,
                "text": "Hold the pose for several breaths, then release and return to your starting position."
            }
        ],
