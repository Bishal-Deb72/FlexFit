        "long_form_content": [],
        "correct_steps": [
            {
                "order": 1,
                "text": "Wrap a loop band around your feet."
            },
            {
                "order": 2,
                "text": "Drive a knee upwards till your thigh is parallel to the ground."
            },
            {
                "order": 3,
                "text": "Pause for a moment and then return to the starting position."
            },
            {
                "order": 4,
                "text": "Try to maintain balance and a good posture when raising the knee."
            }
